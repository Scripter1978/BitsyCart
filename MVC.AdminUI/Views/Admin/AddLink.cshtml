@model MVC.AdminUI.Models.LinkView

@{
    ViewBag.Title = "title";
    Layout = "_LayoutEmpty";
}
<li draggable="true" class="item sortable" id="@Model.Id">
    <div class="row full-width">
        <div class="col-lg-10">
            <span class="my-handle mdi mdi-drag-vertical"></span> @Model.Link
        </div>
        <div class="col-sm-1">
            <button class="" hx-disabled-elt="this" hx-delete='@Url.RouteUrl("Delete", new { id = Model.Id })' hx-target="closest li" hx-swap="delete">
                <span class="my-handle mdi mdi-delete"></span>
            </button>
        </div>
    </div>
</li>